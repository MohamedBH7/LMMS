<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<!-- ASP.NET Core Module -->
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>

		<!-- Forward requests to the Kestrel server -->
		<aspNetCore processPath="dotnet" arguments=".\LMMS.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
	</system.webServer>
</configuration>
